# Base Sepolia RPC URL (required for deployment)
# Get from Alchemy, Infura, or public endpoints like https://sepolia.base.org
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org

# Private key for deployment account (required)
# NEVER commit the actual private key, only use in .env which is gitignored
PRIVATE_KEY=

# Optional: Enable gas reporting
REPORT_GAS=false

# Optional: Enable network forking for local testing
FORKING=false

# Contract addresses (populated after deployment)
MOCK_CCOP_ADDRESS=
WA_USDC_ADDRESS=
FIXED_PRICE_ORACLE_ADDRESS=
